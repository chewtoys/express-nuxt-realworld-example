{
  "name": "express-nuxt-realworld-example",
  "version": "1.0.0",
  "description": "Express and Nuxt.js Realworld Example",
  "author": "gongzza",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently -n TS,Serve \"npm:watch:tsc\" \"npm:watch:serve\"",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run build:nuxt && npm run build:tsc",
    "build:tsc": "tsc -p tsconfig.server.json",
    "build:nuxt": "nuxt build",
    "watch:tsc": "npm run build:tsc -- --watch --pretty",
    "watch:serve": "nodemon --watch dist dist/server",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "generate": "nuxt generate",
    "lint": "npm run lint:es && npm run lint:ts",
    "lint:es": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:ts": "tslint -p tsconfig.json",
    "precommit": "npm run lint",
    "test": "cross-env NODE_ENV=test jest",
    "clean": "rimraf dist && rimraf .nuxt"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.1",
    "bcryptjs": "^2.4.3",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "nuxt": "^2.6.1",
    "pg": "^7.9.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.7.7",
    "slugify": "^1.3.4",
    "ts-node": "^8.0.3",
    "typedi": "^0.8.0",
    "typeorm": "0.2.16",
    "typeorm-typedi-extensions": "^0.2.3"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.6.1",
    "@nuxtjs/auth": "^4.5.3",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/debug": "^4.1.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/faker": "^4.1.5",
    "@types/helmet": "0.0.43",
    "@types/jest": "^24.0.11",
    "@types/js-cookie": "^2.2.2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/marked": "^0.6.5",
    "@types/morgan": "^1.7.35",
    "@types/supertest": "^2.0.7",
    "@types/webpack": "^4.4.27",
    "@typescript-eslint/parser": "^1.6.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.4.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-promise": ">=4.1.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.7.1",
    "marked": "^0.6.2",
    "nodemon": "^1.18.10",
    "nuxt-property-decorator": "^2.1.3",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.1",
    "tslint": "^5.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.1",
    "vue-jest": "^3.0.4"
  }
}
