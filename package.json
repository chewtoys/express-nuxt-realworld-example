{
  "name": "express-nuxt-realworld-example",
  "version": "1.0.0",
  "description": "Express and Nuxt.js Realworld Example",
  "author": "gongzza",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently -n TS,Serve \"npm:watch:tsc\" \"npm:watch:serve\"",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run build:nuxt && npm run build:tsc",
    "build:tsc": "tsc -p tsconfig.server.json",
    "build:nuxt": "nuxt-ts build",
    "watch:tsc": "npm run build:tsc -- --watch --pretty",
    "watch:serve": "nodemon --watch dist dist/server",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "clean": "rimraf dist && rimraf .nuxt"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "nuxt": "^2.4.0",
    "express": "^4.16.4",
    "@nuxtjs/axios": "^5.3.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.5",
    "@types/webpack": "^4.4.24",
    "@typescript-eslint/parser": "^1.3.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jest": ">=21.24.1",
    "eslint-plugin-node": ">=7.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "nuxt-property-decorator": "^2.0.1",
    "nuxt-ts": "^2.4.3",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.3",
    "vue-jest": "^3.0.2"
  }
}
